{
  "name": "codyfight-bots",
  "version": "1.0.0",
  "type": "module",
  "description": "Codyfight internal CAgent bots",
  "main": "src/index.ts",
  "repository": "https://github.com/zvimbalynas/codyfight-bots.git",
  "author": "Codyfight",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node dist/src/index.js",
    "dev": "node --loader ts-node/esm --watch src/index.ts",
    "pre-commit": "yarn format && yarn lint",
    "lint": "yarn eslint --ext .js,.ts,.cjs,.mjs ./src && tsc --noEmit",
    "format": "yarn prettier --config ./.prettierrc './src/**/*.ts' --write"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": ">=12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "codyfight-game-client": "^1.2.14",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "prettier": "^3.4.1"
  }
}
